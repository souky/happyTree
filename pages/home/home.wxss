/* pages/home/home.wxss */
.home_swiper image{
  width: 100%;
  height:480rpx;
}
.home_swiper{
  width: 100%;
  height:480rpx;
}
.home_news{
  margin-top: 20rpx;
}
.last_week_rank{
  width: 99%;
  margin:20rpx auto;
 
}
.last_week_rank .title{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 32rpx;
  color:#666;
  
}
.last_week_rank .contents{
  width: 100%;
  min-height: 60rpx;
  display: flex;
  margin:auto;
  background: #FFB487;
  border-radius: 5rpx;
}
.last_week_rank .contents .items{
  width: 33.33%;
  height: 300rpx;
  margin:50rpx auto auto auto;
  
}
.last_week_rank .contents .items:nth-child(2){
  margin-top:20rpx;
}
.last_week_rank .contents .items:nth-child(2) .maskImg{
  margin-bottom: 50rpx;
}
.last_week_rank .contents .items .maskImg{
  position: relative;
  width: 140rpx;
  height: 140rpx;
  margin: 20rpx auto;
}
.last_week_rank .contents .items .img{
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
  border-radius: 50%;
  
  background: #fff;
}
.last_week_rank .contents .items .img .user{
  width: 140rpx;
  height: 140rpx;
}
.last_week_rank .contents .items .maskImg .prices{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -5rpx;
  bottom: -5rpx;
}
.last_week_rank .contents .items .normal_font{
  width:100%;
  height: 40rpx;
  line-height: 40rpx;
  color:#fff;
}
.last_week_rank .contents .items .empty{
  height: 120rpx;
  line-height: 120rpx;
  color:#fff;
}